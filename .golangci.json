{
	"run": {
		"tests": false,
		"timeout": "5m",
		"go": "1.17"
	},
	"linters": {
		"enable": [
			"asciicheck",
			"bidichk",
			"bodyclose",
			"dogsled",
			"dupl",
			"errcheck",
			"errname",
			"errorlint",
			"exportloopref",
			"forcetypeassert",
			"funlen",
			"gocognit",
			"gocritic",
			"gocyclo",
			"godot",
			"goerr113",
			"gofumpt",
			"goprintffuncname",
			"gosimple",
			"govet",
			"ineffassign",
			"lll",
			"misspell",
			"nakedret",
			"nestif",
			"noctx",
			"nolintlint",
			"nonamedreturns",
			"predeclared",
			"reassign",
			"revive",
			"rowserrcheck",
			"staticcheck",
			"stylecheck",
			"testpackage",
			"typecheck",
			"thelper",
			"unconvert",
			"unparam",
			"unused",
			"usestdlibvars",
			"wastedassign",
			"whitespace"
		]
	},
	"linters-settings": {
		"funlen": {
			"lines": 65
		},
		"nestif": {
			"min-complexity": 9
		},
		"gocritic": {
			"disabled-checks": ["commentFormatting"]
		}
	}
}
